<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>

    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="~/temps/logodoitac/logoicon.jpg" rel="icon">
    <link href="~/temps/logodoitac/logoicon.jpg" rel="apple-touch-icon">
    <title>Nixon : Admin Kit</title>
    <!-- ================= Favicon ================== -->
    <!-- Standard -->
    <link rel="shortcut icon" href="http://placehold.it/64.png/000/fff">
    <!-- Retina iPad Touch Icon-->
    <link rel="apple-touch-icon" sizes="144x144" href="http://placehold.it/144.png/000/fff">
    <!-- Retina iPhone Touch Icon-->
    <link rel="apple-touch-icon" sizes="114x114" href="http://placehold.it/114.png/000/fff">
    <!-- Standard iPad Touch Icon-->
    <link rel="apple-touch-icon" sizes="72x72" href="http://placehold.it/72.png/000/fff">
    <!-- Standard iPhone Touch Icon-->
    <link rel="apple-touch-icon" sizes="57x57" href="http://placehold.it/57.png/000/fff">
    <!-- Styles -->
    <link href="~/tempadmin/assets/fontAwesome/css/fontawesome-all.min.css" rel="stylesheet">
    <link href="~/tempadmin/assets/css/lib/themify-icons.css" rel="stylesheet">
    <link href="~/tempadmin/assets/css/lib/weather-icons.css" rel="stylesheet" />
    <link href="~/tempadmin/assets/css/lib/mmc-chat.css" rel="stylesheet" />
    <link href="~/tempadmin/assets/css/lib/sidebar.css" rel="stylesheet">
    <link href="~/tempadmin/assets/css/lib/bootstrap.min.css" rel="stylesheet">
    <link href="~/tempadmin/assets/css/lib/toastr/toastr.min.css" rel="stylesheet">
    <link href="~/tempadmin/assets/css/lib/nixon.css" rel="stylesheet">
    <link href="~/tempadmin/assets/lib/lobipanel/css/lobipanel.min.css" rel="stylesheet">
    <link href="~/tempadmin/assets/css/style.css" rel="stylesheet">
</head>
<body>
    <div>
        <div class="sidebar sidebar-hide-to-small sidebar-shrink sidebar-gestures">
            <div class="nano">
                <div class="nano-content">
                    <ul>
                        <li class="active">
                            <a class="sidebar-sub-toggle"><i class="ti-home"></i> Hệ thống <span class="sidebar-collapse-icon ti-angle-down"></span></a>
                            <ul>
                                <li><a href="/Admin/sSystem/Index">Thông tin công ty</a></li>
                                <li><a href="/Admin/dUser/Index">Quản lý tài khoản</a></li>
                            </ul>
                        </li>
                        @* <li><a href="app-profile.html"><i class="ti-user"></i> Profile</a></li>
                        <li><a href="app-widget-card.html"><i class="ti-layout-grid2-alt"></i> Widget</a></li> *@
                        <li>
                            <a class="sidebar-sub-toggle"><i class="ti-view-list-alt"></i> Danh mục <span class="sidebar-collapse-icon ti-angle-down"></span></a>
                            <ul>
                                <li><a href="/Admin/cCustomer/Index">Loại đơn vị liên quan</a></li>
                                <li><a href="/Admin/cPost/Index">Loại bài đăng</a></li>
                                <li><a href="/Admin/cProduct/Index">Loại sản phẩm</a></li>
                            </ul>
                        </li>
                        <li>
                            <a class="sidebar-sub-toggle"><i class="ti-panel"></i> Chức năng <span class="sidebar-collapse-icon ti-angle-down"></span></a>
                            <ul>
                                <li><a href="/Admin/dCustomer/Index">Đơn vị liên quan</a></li>
                                <li><a href="/Admin/dProduct/Index">Sản phẩm</a></li>
                                <li><a href="/Admin/dEmployee/Index">Nhân viên</a></li>
                                <li><a href="/Admin/dFeedback/Index">Phản hồi</a></li>
                                <li><a href="/Admin/dPost/Index">Bài đăng</a></li>
                                <li><a href="/Admin/dQnA/index">QnA</a></li>
                                <li><a href="/Admin/dRecuitment/Index">Tuyển dụng</a></li>
                            </ul>
                        </li>
                        <li><a href="app-profile.html"><i class="ti-user"></i> Báo cáo</a></li>
                        @* <li><a target="_blank" href="https://docs.vuejsadmin.com/docs/nixon-admin/"><i class="ti-file"></i> Documentation</a></li> *@
                        @* <li><a><i class="ti-close"></i> Logout</a></li> *@
                    </ul>
                </div>
            </div>
        </div>
        <!-- /# sidebar -->
        <div class="header">
            <div class="pull-left">
                <div class="logo">
                    <a href="index.html">
                        <img id="logoImg" src="~/temps/logodoitac/logoicon.jpg" alt="Quản trị" />
                    </a>
                </div>
                <div class="hamburger sidebar-toggle">
                    <span class="ti-menu"></span>
                </div>
            </div>
            <div class="pull-right p-r-15">
                <ul>
                    <li class="header-icon dib">
                        <i class="ti-bell"></i>
                        <div class="note-count">2</div>
                        <div class="drop-down">
                            <div class="dropdown-content-heading">
                                <span class="text-left">Recent Notifications</span>
                            </div>
                            <div class="dropdown-content-body">
                                <ul>
                                    <li>
                                        <a href="#">
                                            <img class="pull-left m-r-10 avatar-img" src="~/tempadmin/assets/images/avatar/3.jpg" alt="" />
